{
  "name": "rental_management_app",
  "version": "1.0.0",
  "description": "Rental Management API",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production && npm run server:prod",
    "server:prod": "node dist/app.js",
    "build": "babel app.js --out-dir build",
    "clean": "rimraf dist",
    "server": "cross-env DEBUG=app nodemon --exec babel-node app.js",
    "dev": "cross-env NODE_ENV=development npm-run-all server",
    "format": "prettier --write \"./**/*.js\""
  },
  "author": "Jinyeong Lee, Krista Nittmann",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "bcrypt": "^5.0.1",
    "nodemon": "^2.0.7"
  }
}
